end
coroutine.wrap(SPMBBK_fake_script)()
local function EINCWIY_fake_script() -- send2.LocalScript 
	local script = Instance.new('LocalScript', send2)

	local textbox = script.parent.parent.text 
	local time = script.parent.parent.time
	local button = script.Parent
	local message = ""
	local running = false
	
	button.MouseButton1Click:Connect(function()
		if not running then
			running = true
			button.Text = "Start (on)"  -- Change button text when started
			local text = textbox.Text
			local popa = tonumber(time.Text) 
			if popa and popa > 0 then
				message = text
				while running do
					game:GetService("ReplicatedStorage").DefaultChatSystemChatEvents.SayMessageRequest:FireServer(text, "All")
					wait(popa)
				end
			else
				print("Invalid time input. Please enter a positive number.")
			end
		else
			running = false
			button.Text = "Start (off)"  -- Change button text when stopped
		end
	end)
	
end
coroutine.wrap(EINCWIY_fake_script)()
