-- Scripts:

local function XLSVNC_fake_script() -- Frame.drag gui 
	local script = Instance.new('LocalScript', Frame)

	local UserInputService = game:GetService("UserInputService")
	
	local gui = script.Parent
	
	local dragging
	local dragInput
	local dragStart
	local startPos
	
	local function update(input)
		local delta = input.Position - dragStart
		gui.Position = gui:TweenPosition(UDim2.new(startPos.X.Scale, startPos.X.Offset + delta.X, startPos.Y.Scale, startPos.Y.Offset + delta.Y), 'Out', 'Linear', 0, true); -- drag speed
	end
	
	gui.InputBegan:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseButton1 or input.UserInputType == Enum.UserInputType.Touch then
			dragging = true
			dragStart = input.Position
			startPos = gui.Position
	
			input.Changed:Connect(function()
				if input.UserInputState == Enum.UserInputState.End then
					dragging = false
				end
			end)
		end
	end)
	
	gui.InputChanged:Connect(function(input)
		if input.UserInputType == Enum.UserInputType.MouseMovement or input.UserInputType == Enum.UserInputType.Touch then
			dragInput = input
		end
	end)
	
	UserInputService.InputChanged:Connect(function(input)
		if input == dragInput and dragging then
			update(input)
		end
	end)
end
coroutine.wrap(XLSVNC_fake_script)()
local function GKRS_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	
	function zigzag(X) return math.acos(math.cos(X*math.pi))/math.pi end
	
	counter = 0
	
	while wait(0.1)do
		script.Parent.BackgroundColor3 = Color3.fromHSV(zigzag(counter),1,1)
	
		counter = counter + 0.01
	end 
	
end
coroutine.wrap(GKRS_fake_script)()
local function SEMNJG_fake_script() -- Frame.LocalScript 
	local script = Instance.new('LocalScript', Frame)

	print("Made by icecream_kittynew on discord!")
	
end
coroutine.wrap(SEMNJG_fake_script)()
local function CBAO_fake_script() -- send.LocalScript 
	local script = Instance.new('LocalScript', send)

	local textbox = script.parent.parent.text 
	local time = script.parent.parent.time
	local button = script.Parent
	local message = ""
	local running = false
	
	button.MouseButton1Click:Connect(function()
		if not running then
			running = true
			button.Text = "Start (on)"  -- Change button text when started
			local text = textbox.Text
			local popa = tonumber(time.Text) 
			if popa and popa > 0 then
				message = text
				while running do
					local channel = game.TextChatService:WaitForChild("TextChannels"):WaitForChild("RBXGeneral")
					channel:SendAsync(message)
					wait(popa)
				end
			else
				print("Invalid time input. Please enter a positive number.")
			end
		else
			running = false
			button.Text = "Start (off)"  -- Change button text when stopped
		end
	end)
	
end
coroutine.wrap(CBAO_fake_script)()
local function QLTF_fake_script() -- oldchat.LocalScript 
	local script = Instance.new('LocalScript', oldchat)

	script.Parent.MouseButton1Click:Connect(function()
		script.Parent.Visible = false
		script.Parent.Parent["new chat"].Visible = false
		script.Parent.Parent.select.Visible = false
		script.Parent.Parent.send2.Visible = true
		script.Parent.Parent.time.Visible = true
		script.Parent.Parent.text.Visible = true
	end)
end
coroutine.wrap(QLTF_fake_script)()
